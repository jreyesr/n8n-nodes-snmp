{
    "name": "n8n-nodes-snmp",
    "version": "0.0.3",
    "description": "A n8n node that allows communication with SNMP-enabled devices",
    "license": "MIT",
    "homepage": "https://github.com/jreyesr/n8n-nodes-snmp",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "jreyesr"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jreyesr/n8n-nodes-snmp"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [],
        "nodes": [
            "dist/nodes/Snmp/Snmp.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/net-snmp": "^3.23.0",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "net-snmp": "^3.26.0"
    }
}
